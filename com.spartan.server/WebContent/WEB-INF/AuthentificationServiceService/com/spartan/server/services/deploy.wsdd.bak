<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from AuthentificationServiceService WSDL service -->

  <service name="AuthentificationService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://services.server.spartan.com"/>
      <parameter name="wsdlServiceElement" value="AuthentificationServiceService"/>
      <parameter name="schemaQualified" value="http://services.server.spartan.com"/>
      <parameter name="wsdlServicePort" value="AuthentificationService"/>
      <parameter name="className" value="com.spartan.server.services.AuthentificationServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="AuthentificationService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="logIn" qname="operNS:logIn" xmlns:operNS="http://services.server.spartan.com" returnQName="retNS:logInReturn" xmlns:retNS="http://services.server.spartan.com" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:userName" xmlns:pns="http://services.server.spartan.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:password" xmlns:pns="http://services.server.spartan.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="changePassword" qname="operNS:changePassword" xmlns:operNS="http://services.server.spartan.com" returnQName="retNS:changePasswordReturn" xmlns:retNS="http://services.server.spartan.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:userName" xmlns:pns="http://services.server.spartan.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:oldPass" xmlns:pns="http://services.server.spartan.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:newPass" xmlns:pns="http://services.server.spartan.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="logIn changePassword"/>

  </service>
</deployment>
