<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from EventServiceService WSDL service -->

  <service name="EventService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://services.server.spartan.com"/>
      <parameter name="wsdlServiceElement" value="EventServiceService"/>
      <parameter name="schemaQualified" value="http://services.server.spartan.com"/>
      <parameter name="wsdlServicePort" value="EventService"/>
      <parameter name="className" value="com.spartan.server.services.EventServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="EventService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="sumar" qname="operNS:sumar" xmlns:operNS="http://services.server.spartan.com" returnQName="retNS:sumarReturn" xmlns:retNS="http://services.server.spartan.com" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:n1" xmlns:pns="http://services.server.spartan.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:n2" xmlns:pns="http://services.server.spartan.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="sumar"/>

  </service>
</deployment>
